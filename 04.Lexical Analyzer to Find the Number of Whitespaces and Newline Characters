#include <stdio.h>

int main() {
    char str[] = "Hello World \n This is \t Compiler";
    int spaces = 0, newlines = 0;

    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] == ' ') spaces++;
        if (str[i] == '\n') newlines++;
    }

    printf("Spaces: %d\nNewlines: %d\n", spaces, newlines);
    return 0;
}
